proxy-groups:
  - name: 🌎 全球节点
    type: select
    proxies:
      - 🚀 国内节点
      - ⚠️ UDP规则
      - 移动备用节点
      - 备用节点
    use:
      - xiaopro1
      - xiaopro2
      - xiaopro4

  - name: 🚀 国内节点
    type: select
    proxies:
      - 🚀 哔哩免流
      - 🎯 全球直连

  - name: 🚀 哔哩免流
    type: select
    use:
      - bili

  - name: ⚠️ UDP规则
    type: select
    proxies:
      - 🛑 全球拦截
      - 🎯 全球直连

  - name: 🎯 全球直连
    type: select
    proxies:
      - DIRECT

  - name: 🛑 全球拦截
    type: select
    proxies:
      - REJECT

  - name: 🐟 漏网之鱼
    type: relay
    proxies:
      - 🚀 国内节点
      - 🌎 全球节点

  - name: 🔘 AI
    type: relay
    proxies:
      - 🚀 国内节点
      - 备用节点

rules:
  # UDP 规则 
  - NETWORK,UDP,⚠️ UDP规则
  # 广告过滤
  - geosite,category-ads-all,🛑 全球拦截
  # 分流规则
  - DOMAIN-KEYWORD,google,🐟 漏网之鱼
  - DOMAIN-KEYWORD,openai,🔘 AI
  - GEOSITE,CN,🚀 国内节点
  - GEOIP,CN,🚀 国内节点
  - MATCH,🐟 漏网之鱼
