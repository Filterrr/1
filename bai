CF: &CF
    type: vless
    uuid: 1ff64191-83b3-410a-86e3-74f721681a80
    tls: true
    servername: world.kh11a.link
    skip-cert-verify: false
    network: ws
    ws-opts: 
      path: /
      headers: 
        Host: world.kh11a.link

proxies:
  - name: 备用节点
    server: www.gov.se
    port: 443
    <<: *CF

proxy-groups:
  - name: 🌎 全球节点
    type: select
    proxies:
      - 🚀 国内节点
      - ⚠️ UDP规则
      - 备用节点
    use:
      - xiaopro0
      - xiaopro2
      - xiaopro3
      - xiaopro4

  - name: 🚀 国内节点
    type: select
    proxies:
      - 🚀 百度免流
      - 🚀 钉钉免流
      - 🎯 全球直连

  - name: 🚀 百度免流
    type: select
    use:
      - baidu

  - name: 🚀 钉钉免流
    type: select
    use:
      - dingding

  - name: ⚠️ UDP规则
    type: select
    proxies:
      - 🛑 全球拦截
      - 🎯 全球直连

  - name: 🎯 全球直连
    type: select
    proxies:
      - DIRECT

  - name: 🛑 全球拦截
    type: select
    proxies:
      - REJECT

  - name: 🐟 漏网之鱼
    type: relay
    proxies:
      - 🚀 国内节点
      - 🌎 全球节点

rules:
  # UDP 规则 
  - NETWORK,UDP,⚠️ UDP规则
  # 广告过滤
  - geosite,category-ads-all,🛑 全球拦截
  # 分流规则
  - DOMAIN-KEYWORD,google,🐟 漏网之鱼
  - GEOSITE,CN,🚀 国内节点
  - GEOIP,CN,🚀 国内节点
  - MATCH,🐟 漏网之鱼
